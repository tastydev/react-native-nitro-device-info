{
  "name": "react-native-nitro-device-info",
  "version": "1.5.1",
  "description": "Get device info for React Native using Nitro",
  "main": "./lib/module/index.js",
  "types": "./lib/typescript/src/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./lib/typescript/src/index.d.ts",
      "default": "./lib/module/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "cpp",
    "nitrogen",
    "nitro.json",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "prepare": "bob build",
    "nitrogen": "nitrogen",
    "typecheck": "tsc --noEmit",
    "clean": "del-cli lib nitrogen/generated android/build ios/build"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/l2hyunwoo/react-native-nitro-device-info.git"
  },
  "author": "HyunWoo Lee <l2hyunwoo@gmail.com> (https://github.com/l2hyunwoo)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/l2hyunwoo/react-native-nitro-device-info/issues"
  },
  "homepage": "https://github.com/l2hyunwoo/react-native-nitro-device-info#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "0.31.2"
  },
  "devDependencies": {
    "del-cli": "7.0.0",
    "nitrogen": "0.31.2",
    "react-native-builder-bob": "0.40.13",
    "typescript": "5.9.2"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "custom",
        {
          "script": "nitrogen",
          "clean": "nitrogen/"
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "create-react-native-library": {
    "languages": "kotlin-swift",
    "type": "nitro-module",
    "version": "0.54.7"
  }
}
